{
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "css-modules"
  ],
  "extends": [
    "plugin:css-modules/recommended"
  ],
  "globals": {
    "window": true,
    "document": true,
    "navigator": true,
    "fetch": true,
    "describe": true,
    "it": true,
    "expect": true
  },
  "rules": {
    "import/no-cycle": 0,
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-filename-extension": [1, {
      "extensions": [".js", ".jsx"]
    }],
    "import/newline-after-import": [0],
    "eol-last": 0,
    "operator-linebreak": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0,
    "no-multiple-empty-lines": 0,
    "no-use-before-define": 0
  }
}